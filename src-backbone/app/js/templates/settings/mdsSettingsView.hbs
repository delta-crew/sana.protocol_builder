<h4>Update MDS Information</h4>
<form>
  <div class="form-group">
      <label for="mds_link" class="sr-only">{{ i18n "MDS Link" }}</label>
      <input type="text" class="form-control" id="mds_link" name="mds_link" placeholder="MDS">
  </div>

  <div>
      <button type="submit" class="btn btn-primary btn-block" id="submit-btn">{{ i18n "Add MDS Link" }}</button>
  </div>
</form>
